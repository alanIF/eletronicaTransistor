    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       String Vcc1= jTextField1.getText();
       double Vcc = Double.parseDouble(Vcc1);
       String Rc1 =jTextField2.getText();
       double Rc =Double.parseDouble(Rc1);
       String Rb1 =jTextField3.getText();
       double Rb =Double.parseDouble(Rb1);
       String beta1=jTextField4.getText();
       int beta= Integer.parseInt(beta1);
       double icsat= Vcc/Rc;
       double ibsat= icsat/beta;
       double ib = (Vcc - 0.7)/Rb ;
       double ic= ib*beta;
       double Vce=  Vcc-(Rc*ic);
       if((ib>ibsat)||(Vce<0)){
        jLabel7.setText("Saturação");
    }else{
         jLabel7.setText("Ativa");  
       }
       if((Vcc<=0)||(Rc<=0)||(Rb<=0)||(beta<=0)){
           JOptionPane.showMessageDialog(null,"Dados Incorretos");
       }
       jLabel12.setText(""+ic*1000);
       jLabel11.setText(""+Vce);
       jLabel19.setText(""+ib*1000);
       jLabel20.setText(""+ibsat*1000);
       jLabel21.setText(""+icsat*1000);
       
    }                                        

